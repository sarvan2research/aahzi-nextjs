// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model GuestUser {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  mobileNumber String
  course       String
  community    String
  cutoffMarks  Int
  createdAt    DateTime @default(now())
}

model CutOff {
  id                String          @id @default(auto()) @map("_id") @db.ObjectId
  collegeCode       Int
  collegeName       String
  cutoffDetailsList CutoffDetails[]
}

model CutoffDetails {
  id         String  @id @default(auto()) @map("_id") @db.ObjectId
  courseCode String
  courseName String
  year       Int
  cutOffOC   Float
  cutOffBC   Float
  cutOffBCM  Float
  cutOffMBC  Float
  cutOffSC   Float
  cutOffST   Float?
  cutOffSCA  Float
  CutOff     CutOff? @relation(fields: [cutOffId], references: [id])
  cutOffId   String? @db.ObjectId
}

